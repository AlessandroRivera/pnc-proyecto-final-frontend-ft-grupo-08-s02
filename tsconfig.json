{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "jsx": "react-jsx",               
    "moduleResolution": "node",      
    "strict": true,
    "noImplicitAny": false,                
    "esModuleInterop": true,          
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "allowSyntheticDefaultImports": true
  },
  "include": ["src"]            
}
